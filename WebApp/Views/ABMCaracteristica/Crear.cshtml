@{

    ViewBag.Title = "Crear caracteristica";
    Layout = "~/Views/Shared/MasterPanelAdministrador.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <h2>Agregar Caracteristica</h2>
        <form action="/ABMCaracteristica/Crear" method="post">
            <label for="nombre">Nombre del nueva caracteristica:</label>
            <br />
            <input type="text" name="nombre" id="nombre" placeholder="Nombre" />
            <br />
            <br />
            <button type="submit" class="btn btn-danger">Enviar</button>
        </form>
    </div>
</body>
</html>