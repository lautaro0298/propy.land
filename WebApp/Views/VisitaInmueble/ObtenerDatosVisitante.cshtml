@model LibreriaClases.DTO.DTOContactoVisitante
@{
    ViewBag.Title = "ObtenerDatosVisitante";
    Layout = "~/Views/Shared/PanelControl2.cshtml";
}

<div class="container">
    @if (!String.IsNullOrEmpty(ViewBag.Error))
    {
        <div class="alert alert-danger">
            <h4>@ViewBag.Error</h4>
        </div>
    }
    else {
<div class="card">
    <div class="card-header">
        <h4>Datos del visitante</h4>
    </div>
    <div class="card-body">
        <div class="row">
            <div class="col">
                <label>Nombre: <b>@Model.nombreVisitante</b></label>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <label>Email: <b>@Model.email</b></label>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <label>Teléfono: <b>@Model.telefonoContactoPrincipal</b></label>
            </div>
        </div>
    </div>
</div>
    }
    
</div>

