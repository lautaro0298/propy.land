@model IEnumerable<WebApp.Models.Extras>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nombreExtra)
        </th>
        <th>
            Marque opciones
        </th>
        
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombreExtra)
        </td>
        <td>
            <input type="checkbox" name="extras" value="@item.extraId" />
        </td>
        
    </tr>
}

</table>
